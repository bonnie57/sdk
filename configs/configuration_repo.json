{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["enhancement"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["bug"]
    },
    {
      "key": "tests",
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
      "key": "Other",
      "title": "## 💬 Other",
      "labels": ["documentation", "others"]
    },
    {
      "key": "Dependencies",
      "title": "## 📦 Dependencies",
      "labels": ["dependencies"]
    }
  ],
  "ignore_labels": ["ignore"],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "#{{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n#{{UNCATEGORIZED}}\n</details>"
}
