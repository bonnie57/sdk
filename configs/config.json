{
    "template": "#Changes: #{{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n#{{UNCATEGORIZED}}\n</details>",
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": [
                "feat",
                "feature",
                "Feature"
            ]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": [
                "fix",
                "bug",
                "Fix"
            ]
        },
        {
            "title": "## 🛠️ Chores",
            "labels": [
                "chore",
                "Chore"
            ]
        },
        {
            "title": "## 🛠️ Dependencies",
            "labels": [
                "deps"
            ]
        }
    ],
    "label_extractor": [
        {
            "method": "regexr",
            "pattern": "\\[(.+?)\\].+",
            "target": "$1",
            "on_property": "title"
        }
    ]
}
}